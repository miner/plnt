#!/bin/sh
PDB=~/plants/data/plantdb
if [ ! -e $PDB ]; then echo "missing $PDB"; exit -1; fi
cd $PDB
ls -1Rs * >/tmp/new-pdb-report.txt
cd ../web-plantdb
ls -1Rs * >/tmp/web-pdb-report.txt
opendiff /tmp/web-pdb-report.txt  /tmp/new-pdb-report.txt

opendiff ~/plants/data/web-plantdb/1nfo ~/plants/data/plantdb/1nfo
opendiff ~/plants/data/web-plantdb/3ook ~/plants/data/plantdb/3ook
#opendiff ~/plants/data/web-plantdb ~/plants/data/plantdb
